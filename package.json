{
  "name": "@bird-studio/js-config",
  "version": "3.0.0",
  "description": "config files",
  "keywords": [
    "config",
    "conventional-commits",
    "conventional",
    "conventional changelog",
    "semantic-version",
    "semantic-release"
  ],
  "homepage": "https://github.com/bird-studio/js-config",
  "repository": "bird-studio/js-config",
  "license": "MIT",
  "author": "akira-toriyama",
  "main": "lib/index.js",
  "files": [
    "/lib",
    "/tsConfig",
    "README.md"
  ],
  "scripts": {
    "qualityCheck": "run-s qualityCheck:*",
    "qualityCheck:esLint": "eslint --ignore-path .gitignore .",
    "qualityCheck:fmt": "prettier  --check --ignore-path .gitignore .",
    "prepack": "yarn qualityCheck",
    "prepare": "git config --local core.hooksPath .githooks"
  },
  "dependencies": {
    "cosmiconfig": "7.0.1"
  },
  "devDependencies": {
    "@bird-studio/interactive-message": "3.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.1",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "semantic-release": "19.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
